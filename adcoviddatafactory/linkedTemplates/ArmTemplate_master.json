{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adcoviddatafactory"
		},
		"ls_source_sa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_source_sa'"
		},
		"ls_test_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_test'"
		},
		"ls_http_opendata_ecdc_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseUrl}"
		},
		"ls_sadestination_dl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://destinationsaadf.dfs.core.windows.net"
		},
		"trigger1_properties_pl_ingest_cases_deaths_data_parameters_sourceRelativeUrl": {
			"type": "string",
			"defaultValue": "sabideep/covid19/blob/main/ecdc_data/testing.csv"
		},
		"trigger1_properties_pl_ingest_cases_deaths_data_parameters_sinkFileName": {
			"type": "string",
			"defaultValue": "testing.csv"
		},
		"ls_keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "@{linkedService().devKeyVault}"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adcoviddatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_source_sa_connectionString": {
						"value": "[parameters('ls_source_sa_connectionString')]"
					},
					"ls_test_connectionString": {
						"value": "[parameters('ls_test_connectionString')]"
					},
					"ls_http_opendata_ecdc_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_ecdc_properties_typeProperties_url')]"
					},
					"ls_keyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_keyvault_properties_typeProperties_baseUrl')]"
					},
					"ls_sadestination_dl_properties_typeProperties_url": {
						"value": "[parameters('ls_sadestination_dl_properties_typeProperties_url')]"
					},
					"trigger1_properties_pl_ingest_cases_deaths_data_parameters_sourceRelativeUrl": {
						"value": "[parameters('trigger1_properties_pl_ingest_cases_deaths_data_parameters_sourceRelativeUrl')]"
					},
					"trigger1_properties_pl_ingest_cases_deaths_data_parameters_sinkFileName": {
						"value": "[parameters('trigger1_properties_pl_ingest_cases_deaths_data_parameters_sinkFileName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}